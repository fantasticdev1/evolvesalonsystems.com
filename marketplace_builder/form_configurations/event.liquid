---
name: event
resource: Customization
resource_owner: anyone
configuration:
  properties:
    image_url:
    title:
    description:
    detail_title:
    detail_description:
    start_date:
    end_date:
    start_time:
    end_time:
    organization:
    address:
    investment:
    phone:
---
{% comment %} {% assign f = form_builder.fields.properties -%}
{% form -%}
	{% include 'forms/fieldGroup', field: f.image_url, type: 'text', label: 'ImageURL' -%}
  {% include 'forms/fieldGroup', field: f.title, type: 'text', label: 'Title' -%}
  {% include 'forms/fieldGroup', field: f.description, type: 'textarea', label: 'Description' -%}
	{% include 'forms/fieldGroup', field: f.detail_title, type: 'text', label: 'Detail Title' -%}
  {% include 'forms/fieldGroup', field: f.detail_description, type: 'textarea', label: 'Detail Description' -%}
  {% include 'forms/fieldGroup', field: f.start_date, type: 'text', label: 'Start Date' -%}
  {% include 'forms/fieldGroup', field: f.end_date, type: 'text', label: 'End Date' -%}
  {% include 'forms/fieldGroup', field: f.start_time, type: 'text', label: 'Start Time' -%}
  {% include 'forms/fieldGroup', field: f.end_time, type: 'text', label: 'End Time' -%}
  {% include 'forms/fieldGroup', field: f.organization, type: 'text', label: 'Organization' -%}
  {% include 'forms/fieldGroup', field: f.address, type: 'text', label: 'Address' -%}
  {% include 'forms/fieldGroup', field: f.investment, type: 'text', label: 'Investment' -%}
  {% include 'forms/fieldGroup', field: f.phone, type: 'text', label: 'Phone' -%}
	<input type="submit" class="button">
{% endform -%}

<script>console.log({{form|json}})</script> {% endcomment %}
